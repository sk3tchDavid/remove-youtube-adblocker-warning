{
  "manifest_version": 2,
  "name": "youtube adblocker warning bypass",
  "version": "1.0",
  "description": "",
  "permissions": [
    "activeTab",
    "webNavigation",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
